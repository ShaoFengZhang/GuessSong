.bg {
    background-image: url('https://www.vryzy.cn/storage/web/indexbg1.png');
    position: relative;
}

.pullnewbox {
    position: absolute;
    top: 380rpx;
    right: 0;
}

.pullnewbox image, .pullnewbox {
    width: 150rpx;
    height: 150rpx;
}

.pullnewboxtext {
    width: 100%;
    height: 20rpx;
    position: absolute;
    bottom: 52rpx;
    left: 0rpx;
    font-size: 26rpx;
    text-align: center;
}

.adonebox image, .adtwobox image {
    width: 150rpx;
    height: 150rpx;
}

.adonebox, .adtwobox {
    position: absolute;
    top: 200rpx;
}

.adonebox {
    top: 200rpx;
    left: 30rpx;
    animation: adonebox 0.5s ease-in-out infinite;
}

.adtwobox {
    /* animation: adonebox 0.5s ease-in-out infinite;  */
}

@keyframes adonebox {
    0% {
        transform: rotate(0deg);
    }

    50% {
        transform: rotate(20deg);
    }

    100% {
        transform: rotate(0deg);
    }
}

.adtwobox {
    right: 30rpx;
}

.topicon image, .topicon {
    width: 331rpx;
    height: 386rpx;
    margin: 0 auto;
}

.topruletext {
    width: 100%;
    text-align: center;
    font-size: 30rpx;
    color: rgba(57, 57, 57, 1);
    margin-top: 2.2vh;
}

.startBtn {
    margin: 2.2vh auto 0;
    position: relative;
}

.startBtn image, .startBtn {
    width: 514rpx;
    height: 115rpx;
}

#formID {
    width: 514rpx;
    height: 115rpx;
    background-color: rgba(0, 0, 0, 0);
    padding: 0;
}

.startBtn #starbutton {
    width: 514rpx;
    height: 115rpx;
    position: absolute;
    top: 0;
    left: 0;
    background-color: #fff;
    margin-top: 0;
    opacity: 0;
}

.adindexthree {
    margin: 3.4vh auto 0;
    position: relative;
}

.adindexthree, .adindexthree image, .AdThreeNav {
    width: 514rpx;
    height: 106rpx;
}

.threeADbox {
    width: 100%;
    /* height: 120rpx; */
    padding: 0 66rpx;
    box-sizing: border-box;
    margin-top: 2.9vh;
    display: flex;
    justify-content: space-around;
}

.threeADbox image, .AdThreeBoxNav {
    width: 170rpx;
    height: 120rpx;
    border-radius: 18rpx;
}

.dynamicAdvice {
    margin-top: 3.5vh;
}

.dynamicAdvice image, .dynamicAdviceNav {
    width: 750rpx;
    height: 180rpx;
    margin-top: 10rpx;
}

.ruleMask {
    width: 100%;
    height: 100vh;
    background-color: rgba(0, 0, 0, 0.46);
    overflow: hidden;
    position: absolute;
    top: 0;
    left: 0;
}

.closeimg {
    width: 38rpx;
    height: 38rpx;
    position: absolute;
    top: 20rpx;
    right: 30rpx;
    z-index: 10;
}

.ruleMask .closeimg image {
    width: 38rpx;
    height: 38rpx;
}

.ruleimg {
    width: 100%;
    /* height: 750rpx; */
    float: right;
}

.ruleContent {
    width: 568rpx;
    height: 686rpx;
    background-color: rgba(255, 254, 246, 1);
    margin: 15vh auto 0;
    border-radius: 18rpx;
    box-sizing: border-box;
    padding-top: 10rpx;
    position: relative;
}

.nochanceMask {
    padding: 40rpx 0 0 0;
}

.nochancecloseimg {
    width: 38rpx;
    height: 38rpx;
    position: absolute;
    top: 10rpx;
    right: 10rpx;
}

